
{% extends 'layoutForm.twig' %}
{% block title %}Generar Cedula{% endblock %}
{% block form %}
    <form class="form" id="DocumentosSiglas" method="POST" action="/SIA/juridico/CedulaIrac/add">
        <div class="datos-llenado">
            <div class="form-group siglas row">
                <label class="form-control-label col-lg-2" for="siglas">Siglas</label>
                <input class="form-control col-lg-2" id="siglas" type="text" name="siglas" required=""/>
                <input id="idSubTipoDocumento" type="hidden" name="idSubTipoDocumento" value="{{ idSubTipoDocumento }}" />
                <input type="hidden" name="idVolante" value="{{ idVolante }}"/>
            </div>
            <div class="form-group fecha row">
                <label class="form-control-label col-lg-2" for="fecha">Fecha Documento</label>
                <input class="form-control fechaInput col-lg-2" id="fOficio" type="text" name="fOficio" required="" pattern="(?:19|20)[0-9]{2}-(?:(?:0[1-9]|1[0-2])-(?:0[1-9]|1[0-9]|2[0-9])|(?:(?!02)(?:0[1-9]|1[0-2])-(?:30))|(?:(?:0[13578]|1[02])-31))"/>
            </div>
            <div class="form-group firmas row">
                <label class="form-control-label col-lg-2" for="numFolio">AÃ±adir Firmas</label>
                <button class="btn btn-info col-lg-2" id="modalFirmas">Firmas</button>
                <input type="hidden" name="idPuestosJuridico" id="idPuestosJuridico">
            </div>
            <div class="form-group numFolio row">
                <label class="form-control-label col-lg-2" for="numFolio">Numero Folio</label>
                <input class="form-control col-lg-2" id="numFolio" type="text" name="numFolio" required=""/>
            </div>

        </div>


        <div class="form-group send">
            <input class="btn btn-primary btn-sm" type="submit" value="Guardar"/>
            <button class="btn btn-danger" id="cancelar">Cancelar</button>
        </div>
    </form>
    {% if err %}
        <div class="alert alert-danger alert-dismissible fade show" role="alert">
            <strong>Error!</strong> {{ err }}
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    {% endif %}
{% endblock %}









